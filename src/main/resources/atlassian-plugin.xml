<?xml version="1.0" encoding="UTF-8"?>
<atlassian-plugin key="${atlassian.plugin.key}"
	name="${project.name}" pluginsVersion="2">
	<plugin-info>
		<version>${project.version}</version>
	</plugin-info>

	<resource
		type="download"
		name="jira-versions-thumbnail.png"
		location="jira-versions-thumbnail.png"/>

	<web-resource key="resources">
		<resource type="download" name="jira-versions.js"
			location="jira-versions.js">
			<property key="content-type" value="text/javascript" />
		</resource>
		<resource type="download" name="theme.default.css"
			location="theme.default.css">
			<property key="content-type" value="text/css" />
		</resource>
	</web-resource>

	<gadget key="jira-versions" location="jira-versions.xml" />
</atlassian-plugin>
